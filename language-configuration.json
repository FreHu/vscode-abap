{
    "comments": {
        "lineComment": "\""
    },
    "autoClosingPairs": [
        [
            "( ",
            " )"
        ],
        [
            "[ ",
            " ]"
        ],
        [
            "<",
            ">"
        ]
    ],
    "surroundingPairs": [
        [
            "`",
            "`"
        ],
        [
            "'",
            "'"
        ],
        [
            "|",
            "|"
        ],
        [
            "<",
            ">"
        ]
    ],
    "indentationRules": {
      "increaseIndentPattern": {
        "pattern": "^\\s*(METHOD|IF|LOOP|CLASS|CASE|DO|FORM|ELSE|WHILE|TRY|CATCH).*\\.$",
        "flags": "i"
      },
      "decreaseIndentPattern": {
        "pattern": "^\\s*(ENDMETHOD|ENDIF|ENDLOOP|ENDCLASS|ENDCASE|ENDDO|ENDFORM|ELSE|ENDWHILE|ENDTRY|CATCH)\\.$",
        "flags": "i"
      },
    }
}